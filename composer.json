{
    "name": "mvqn/robo-tasks",
    "description": "Some common Robo Tasks.",
    "type": "robo-tasks",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Spaeth",
            "email": "rspaeth@mvqn.net"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "ext-zip": "*",
        "rspaeth/sftp": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~9",
        "consolidation/robo": "^1.4",
        "rspaeth/common": "*"
    },
    "autoload": {
        "psr-4": {
            "rspaeth\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "rspaeth\\": "./tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        { "type": "path", "url": "../*"}
    ]
}
